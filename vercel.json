{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "style.css",
      "use": "@vercel/static"
    },
    {
      "src": "script.js",
      "use": "@vercel/static"
    },
    {
      "src": "api/generate.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/create-checkout.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/create-one-time-checkout.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/generate",
      "dest": "/api/generate.js"
    },
    {
      "src": "/api/create-checkout", 
      "dest": "/api/create-checkout.js"
    },
    {
      "src": "/api/create-one-time-checkout",
      "dest": "/api/create-one-time-checkout.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1.html"
    }
  ]
}
